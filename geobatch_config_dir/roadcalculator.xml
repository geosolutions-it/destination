            <name>Connessione DB</name>
            <description>Connessione DB</description>

            <listenerConfigurations/>
            <failIgnored>false</failIgnored>
			
            <outputFeature>
                <dataStore>
                   <entry>
                     <string>dbtype</string>
                     <string>postgis</string>
                   </entry>
                   <entry>
                     <string>host</string>
                     <string>localhost</string>
                   </entry>
                   <entry>
                     <string>port</string>
